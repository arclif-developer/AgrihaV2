    "moment": "^2.29.4",
    "mongoose": "^6.8.3",
    "nestjs-razorpay": "^1.0.3",
    "nestjs-twilio": "^3.2.1",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "randomstring": "^1.2.3",
